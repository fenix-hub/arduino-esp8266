; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="ArduinoESP8266"
run/main_scene="res://scenes/MainScene.tscn"
config/icon="res://app_icon.png"

[autoload]

webserver_data="*res://scenes/scripts/webserver_data.gd"
HTTPManager="*res://scenes/HTTPManager.tscn"

[display]

window/size/width=576
window/size/height=1024
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
quality/filters/use_nearest_mipmap_filter=true
environment/default_environment="res://default_env.tres"
